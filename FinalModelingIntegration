  xgb = XGBRegressor(\n","        n_estimators=500,\n","        learning_rate=0.05,\n","        max_depth=6,\n","        subsample=0.8,\n","        colsample_bytree=0.8,\n","        objective=\"reg:squarederror\",\n","        random_state=42,\n","        tree_method=\"hist\"  # FAST for large datasets\n","    )\n","\n","    xgb.fit(X_train, y_train)\n","    preds_xgb = xgb.predict(X_test)\n","\n","    rmse = mean_squared_error(y_test, preds_xgb)\n","    mape = mean_absolute_percentage_error(y_test, preds_xgb)\n","    mae = mean_absolute_error(y_test, preds_xgb)\n","\n","    results[\"XGBoost\"].append((rmse, mape, mae))\n","\n","    print(f\"XGBoost:      RMSE={rmse:.4f},  MAPE={mape:.4f},  MAE={mae:.4f}\")\n","# Random Forest\n","    rf = RandomForestRegressor(\n","        n_estimators=400,\n","        max_depth=None,\n","        random_state=42\n","    )\n","    rf.fit(X_train, y_train)\n","    preds_rf = rf.predict(X_test)\n","\n","    rmse = mean_squared_error(y_test, preds_rf)\n","    mape = mean_absolute_percentage_error(y_test, preds_rf)\n","    mae = mean_absolute_error(y_test, preds_rf)\n","\n","    results[\"RandomForest\"].append((rmse, mape, mae))\n","\n","    print(f\"RandomForest: RMSE={rmse:.4f},  MAPE={mape:.4f},  MAE={mae:.4f}\")\n","\n","# LASSO Regression\n","    lasso = Lasso(alpha=0.01, max_iter=10000, random_state=42)\n","    lasso.fit(X_train_scaled, y_train)\n","    preds_lasso = lasso.predict(X_test_scaled)\n","\n","    rmse = mean_squared_error(y_test, preds_lasso)\n","    mape = mean_absolute_percentage_error(y_test, preds_lasso)\n","    mae = mean_absolute_error(y_test, preds_lasso)\n","\n","    results[\"Lasso\"].append((rmse, mape, mae))\n","\n","    print(f\"LASSO:        RMSE={rmse:.4f},  MAPE={mape:.4f},  MAE={mae:.4f}\")\n","\n","# Final Summary Output\n","print(\"\\n\\n=== FINAL AVERAGE METRICS ACROSS FOLDS ===\")\n","\n","avg_metrics = {}\n","\n","for model_name, metrics in results.items():\n","    rmses = [m[0] for m in metrics]\n","    mapes = [m[1] for m in metrics]\n","    maes = [m[2] for m in metrics]\n","    avg_metrics[model_name] = {\n","        \"Average RMSE\": np.mean(rmses),\n","        \"Average MAPE\": np.mean(mapes),\n","        \"Average MAE\": np.mean(maes)\n","    }\n","\n","    print(f\"\\n{model_name}:\")\n","    print(f\"Average RMSE: {np.mean(rmses):.4f}\")\n","    print(f\"Average MAPE: {np.mean(mapes):.4f}\")\n","    print(f\"Average MAE:  {np.mean(maes):.4f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"j0qQclwncBia","executionInfo":{"status":"ok","timestamp":1765212530357,"user_tz":300,"elapsed":41390,"user":{"displayName":"Matt Collins","userId":"05613590412374382374"}},"outputId":"2c30d90c-161b-4953-be53-3883a3284266"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Fold 1\n","XGBoost:      RMSE=0.6521,  MAPE=0.2785,  MAE=0.6621\n","RandomForest: RMSE=0.6536,  MAPE=0.2809,  MAE=0.6667\n","LASSO:        RMSE=0.3515,  MAPE=0.2009,  MAE=0.4674\n","Fold 2\n","XGBoost:      RMSE=0.4507,  MAPE=0.1635,  MAE=0.5818\n","RandomForest: RMSE=0.4337,  MAPE=0.1620,  MAE=0.5788\n","LASSO:        RMSE=0.5241,  MAPE=0.1947,  MAE=0.6912\n","Fold 3\n","XGBoost:      RMSE=0.7695,  MAPE=0.3465,  MAE=0.7774\n","RandomForest: RMSE=0.7389,  MAPE=0.3405,  MAE=0.7610\n","LASSO:        RMSE=0.5897,  MAPE=0.3093,  MAE=0.6968\n","Fold 4\n","XGBoost:      RMSE=0.1396,  MAPE=0.1183,  MAE=0.2992\n","RandomForest: RMSE=0.1296,  MAPE=0.1161,  MAE=0.2951\n","LASSO:        RMSE=0.1750,  MAPE=0.1526,  MAE=0.3618\n","Fold 5\n","XGBoost:      RMSE=0.5442,  MAPE=0.2163,  MAE=0.5830\n","RandomForest: RMSE=0.5229,  MAPE=0.2129,  MAE=0.5700\n","LASSO:        RMSE=0.3953,  MAPE=0.2073,  MAE=0.5285\n","\n","\n","=== FINAL AVERAGE METRICS ACROSS FOLDS ===\n","\n","XGBoost:\n","Average RMSE: 0.5112\n","Average MAPE: 0.2246\n","Average MAE:   0.5807\n","\n","RandomForest:\n","Average RMSE: 0.4958\n","Average MAPE: 0.2225\n","Average MAE:   0.5743\n","\