_state=42\n","    )\n","    rf.fit(X_train, y_train)\n","    preds_rf = rf.predict(X_test)\n","\n","    rmse = mean_squared_error(y_test, preds_rf)\n","    mape = mean_absolute_percentage_error(y_test, preds_rf)\n","    mae = mean_absolute_percentage_error(y_test, preds_rf)\n","\n","    results[\"RandomForest\"].append((rmse, mape, mae))\n","\n","    print(f\"RandomForest: RMSE={rmse:.4f},  MAPE={mape:.4f},  MAE={mae:.4f}\")\n","\n","# LASSO Regression\n","    lasso = Lasso(alpha=0.01, max_iter=10000, random_state=42)\n","    lasso.fit(X_train_scaled, y_train)\n","    preds_lasso = lasso.predict(X_test_scaled)\n","\n","    rmse = mean_squared_error(y_test, preds_lasso)\n","    mape = mean_absolute_percentage_error(y_test, preds_lasso)\n","    mae = mean_absolute_error(y_test, preds_lasso)\n","\n","    results[\"Lasso\"].append((rmse, mape, mae))\n","\n","    print(f\"LASSO:        RMSE={rmse:.4f},  MAPE={mape:.4f},  MAE={mae:.4f}\")\n","\n","# Final Summary Output\n","print(\"\\n\\n=== FINAL AVERAGE METRICS ACROSS FOLDS ===\")\n","\n","avg_metrics = {}\n","\n","for model_name, metrics in results.items():\n","    rmses = [m[0] for m in metrics]\n","    mapes = [m[1] for m in metrics]\n","    maes = [m[2] for m in metrics]\n","    avg_metrics[model_name] = {\n","        \"Average RMSE\": np.mean(rmses),\n","        \"Average MAPE\": np.mean(mapes),\n","        \"Average MAE\": np.mean(maes)\n","    }\n","\n","    print(f\"\\n{model_name}:\")\n","    print(f\"Average RMSE: {np.mean(rmses):.4f}\")\n","    print(f\"Average MAPE: {np.mean(mapes):.4f}\")\n","    print(f\"Average MAE:  {np.mean(maes):.4f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"-zOTMFJckQhN","executionInfo":{"status":"ok","timestamp":1765212715394,"user_tz":300,"elapsed":4701,"user":{"displayName":"Matt Collins","userId":"05613590412374382374"}},"outputId":"747fcf8a-3556-4146-dfd1-f7cab65e7fe9"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["